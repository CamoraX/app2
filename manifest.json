{
  "name": "My app",
  "default_locale": "en",
  "description": "test",
  "version": "3.0.0",
  "manifest_version": 2,
  "host_permissions":["<all_urls>"],
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "browsingData",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Content-Security-Policy headers are enabled",
    "default_icon": {
      "16": "images/icon38-off.png"
    }
  },
  "icons": {
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
